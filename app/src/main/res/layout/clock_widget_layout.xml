<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/custom_clock_widget"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:padding="@dimen/clock_margin"
    android:gravity="center" >

    <AnalogClock
        android:id="@+id/AnalogClock0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:dial="@drawable/clock_dial_roman_old_black"
        android:hand_hour="@drawable/clock_hour_hand_roman_white"
        android:hand_minute="@drawable/clock_minute_hand_roman_white"
        android:visibility="invisible"
        />
