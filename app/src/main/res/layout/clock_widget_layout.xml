        android:id="@+id/AnalogClock2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:dial="@drawable/clock_dial_roman_old_black"
        android:hand_hour="@drawable/clock_hour_hand_roman_red"
        android:hand_minute="@drawable/clock_minute_hand_roman_red"
        android:visibility="invisible"
        />
